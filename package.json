{
  "name": "tone-console",
  "version": "0.1.0",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "pnpm gen:version",
    "dev": "vite",
    "build": "tsc && vite build",
    "gen:version": "esno scripts/version_info.ts",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "pnpm lint --fix && pnpm prettier --write",
    "prettier": "prettier \"**/*.{vue,js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "commit": "git-cz",
    "deploy:prod": "pnpm rimraf serverless.yaml && cp infra/serverless/tencent_cloud_prod.yaml serverless.yaml && sls deploy -s prod && pnpm rimraf serverless.yaml"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@mantine/carousel": "^7.5.3",
    "@mantine/charts": "^7.5.3",
    "@mantine/code-highlight": "^7.5.3",
    "@mantine/core": "^7.5.3",
    "@mantine/dates": "^7.5.3",
    "@mantine/dropzone": "^7.5.3",
    "@mantine/form": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@mantine/modals": "^7.5.3",
    "@mantine/notifications": "^7.5.3",
    "@mantine/nprogress": "^7.5.3",
    "@mantine/spotlight": "^7.5.3",
    "@mantine/tiptap": "^7.5.3",
    "@popperjs/core": "^2.11.8",
    "@tabler/icons-react": "^2.47.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@tanstack/react-table": "^8.12.0",
    "@tiptap/extension-link": "^2.2.3",
    "@tiptap/react": "^2.2.3",
    "@tiptap/starter-kit": "^2.2.3",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "8.0.0-rc22",
    "esno": "^4.0.0",
    "events": "^3.3.0",
    "framer-motion": "^11.0.5",
    "i18next": "^23.9.0",
    "jsencrypt": "^3.3.2",
    "lucide-react": "^0.334.0",
    "mantine-datatable": "^7.5.1",
    "ofetch": "^1.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.0.5",
    "react-router": "^6.22.1",
    "react-router-dom": "^6.22.1",
    "react-transition-group": "^4.4.5",
    "recharts": "2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/loadable__component": "^5.13.8",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "postcss-nesting": "^12.0.2",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^1.3.0"
  },
  "packageManager": "pnpm@7.27.0",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT"
}
