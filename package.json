{
  "name": "@stocms/console",
  "version": "0.1.0",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "pnpm gen:version",
    "dev": "vite",
    "build": "tsc && vite build",
    "gen:version": "esno scripts/version_info.ts",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "pnpm lint --fix && pnpm prettier --write",
    "prettier": "prettier \"**/*.{vue,js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "commit": "git-cz",
    "deploy:prod": "pnpm rimraf serverless.yaml && cp infra/serverless/tencent_cloud_prod.yaml serverless.yaml && sls deploy -s prod && pnpm rimraf serverless.yaml"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@mantine/carousel": "^6.0.13",
    "@mantine/core": "^6.0.13",
    "@mantine/dates": "^6.0.13",
    "@mantine/dropzone": "^6.0.13",
    "@mantine/form": "^6.0.13",
    "@mantine/hooks": "^6.0.13",
    "@mantine/modals": "^6.0.13",
    "@mantine/notifications": "^6.0.13",
    "@mantine/nprogress": "^6.0.13",
    "@mantine/prism": "^6.0.13",
    "@mantine/spotlight": "^6.0.13",
    "@mantine/tiptap": "^6.0.13",
    "@popperjs/core": "^2.11.8",
    "@tabler/icons-react": "^2.21.0",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "esno": "^0.16.3",
    "i18next": "^22.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.9",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.11.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.16.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT"
}
