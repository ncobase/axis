{
  "name": "@stocms/console",
  "version": "0.1.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "pnpm lint --fix && pnpm prettier --write",
    "prettier": "prettier \"**/*.{vue,js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "commit": "git-cz",
    "deploy:prod": "pnpm rimraf serverless.yaml && cp infra/serverless/tencent_cloud_prod.yaml serverless.yaml && sls deploy -s prod && pnpm rimraf serverless.yaml"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@formiz/core": "^1.8.1",
    "@formiz/validations": "^1.0.0",
    "@loadable/component": "^5.15.3",
    "@mantine/carousel": "^5.10.5",
    "@mantine/core": "^5.10.5",
    "@mantine/dates": "^5.10.5",
    "@mantine/dropzone": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@mantine/modals": "^5.10.5",
    "@mantine/next": "^5.10.5",
    "@mantine/notifications": "^5.10.5",
    "@mantine/nprogress": "^5.10.5",
    "@mantine/prism": "^5.10.5",
    "@mantine/rte": "^5.10.5",
    "@mantine/spotlight": "^5.10.5",
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-query": "^4.24.10",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "embla-carousel-react": "^7.0.9",
    "i18next": "^22.4.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-query": "^3.39.3",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/babel__core": "^7.20.0",
    "@types/ejs": "^3.1.2",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^18.14.6",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.14.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT"
}
