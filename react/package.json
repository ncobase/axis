    "@ncobase/tsconfig": "workspace:*",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-syntax-highlighter": "catalog:",
    "react": "catalog:",
    "tsup": "catalog:"
