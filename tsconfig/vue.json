{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue 3",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "vue",
      "vite/client"
    ],
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true
  }
}
