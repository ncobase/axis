{
  "name": "@ncobase/tailwind",
  "version": "0.2.0",
  "description": "Common tailwindcss configuration with v3 and v4 compatibility",
  "type": "module",
  "main": "./mjs/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./mjs/index.mjs",
      "require": "./cjs/index.cjs",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "mjs",
    "cjs",
    "types"
  ],
  "peerDependencies": {
    "tailwindcss": "^3.4.0 || ^4.0.0",
    "postcss": "^8.5.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/postcss-import": "^14.0.3"
  }
}
